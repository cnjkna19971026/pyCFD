    }
    heater_to_topAir
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          40;
        startFace       172;
        sampleMode      nearestPatchFace;
        sampleRegion    topAir;
        samplePatch     topAir_to_heater;
    }
    heater_to_rightSolid
    {
        type            mappedWall;
        inGroups        1(wall);
        nFaces          10;
        startFace       212;
        sampleMode      nearestPatchFace;
