FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/heater";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    minY
    {
        type            zeroGradient;
    }
    minZ
    {
        type            zeroGradient;
    }
    maxZ
    {
        type            zeroGradient;
    }
    heater_to_topAir
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_rightSolid
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_leftSolid
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_bottomAir
    {
        type            calculated;
        value           uniform 0;
    }
}


